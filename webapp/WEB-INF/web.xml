<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>it.alexandria.hibernate.control.Login</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Library</servlet-name>
		<servlet-class>it.alexandria.hibernate.control.GestioneLibreria</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Search</servlet-name>
		<servlet-class>it.alexandria.hibernate.control.Ricerca</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Resource</servlet-name>
		<servlet-class>it.alexandria.hibernate.control.GestioneRisorsa</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Profile</servlet-name>
		<servlet-class>it.alexandria.hibernate.control.GestioneProfilo</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Messaggi</servlet-name>
		<servlet-class>it.alexandria.hibernate.control.GestioneCasella</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Messaggi</servlet-name>
		<url-pattern>/boxmessages</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Profile</servlet-name>
		<url-pattern>/profile</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Search</servlet-name>
		<url-pattern>/search</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Resource</servlet-name>
		<url-pattern>/resource</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Library</servlet-name>
		<url-pattern>/library</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/loginsubmit</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
	</welcome-file-list>
	
</web-app>
